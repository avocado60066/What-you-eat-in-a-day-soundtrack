.custom-container {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 90%;
  margin: 0 auto;
}

.custom-image {
  width: 40%; /* 图片占40%宽度 */
  text-align: right;
  margin-left: 80px;
}

 
.custom-text {
  font-family: Lucida Grande,Lucida Sans Unicode,Lucida Sans,Geneva,Verdana,sans-serif; 
  font-size: 18px;
  color: black; /* 设置字体颜色为黑色 */
  width: 100%; 
}


.custom-font {
    font-family: Lucida Grande,Lucida Sans Unicode,Lucida Sans,Geneva,Verdana,sans-serif; 
    font-size: 18px;
    color: black; 
    text-align: center; 
   display: block; /* 将元素设置为块级元素 */
    width: 100%; 
    margin: 20 auto;
}

.chooses{
  	font-family: Verdana,Geneva,sans-serif; 
    color: white; 
    text-align: center;
	font-size: 50px;
}

@keyframes blink {
  0%, 100% {
    color: black;
  }
  50% {
    color: white;
  }
}


tw-link {
  color: black;
  animation: blink 1s ease-in-out infinite;
}


tw-link[tags~="Start"]{
    color: white;
  	animation: blink 1s ease-in-out infinite;
    font-family: Verdana,Geneva,sans-serif; 
	font-size: 50px;
}

tw-passage {
  text-align: center;
  color:black;
  font-family: Lucida Grande,Lucida Sans Unicode,Lucida Sans,Geneva,Verdana,sans-serif; 
  font-size: 18px;
}

tw-passage[tags~="black"] {
  text-align: center;
  color:white;
  font-family: Lucida Grande,Lucida Sans Unicode,Lucida Sans,Geneva,Verdana,sans-serif; 
  font-size: 18px;
}

hr {
  height: 2px;
  background-color: black;
  border: none;
}

.outerBackground {
    background-color: rgba(255, 255, 255, 0.7);
    padding: 50px;
    border-radius: 10px;
}

.center {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100vh; 
}

.startButton {
    background-color: rgba(0, 0, 0, 1);
    padding: 50px;
    border-radius: 10px;
  	display: flex;
  	justify-content: center;
  	align-items: center;
}

.startText {
  	color: white;
  	animation: blink 1s ease-in-out infinite;
    	font-family: Verdana,Geneva,sans-serif; 
	font-size: 50px;
}

.scores {
    font-weight: 700;
    background-color: rgba(0, 0, 0, 0.1);
    padding: 50px;
    border-radius: 10px;
}

a{
  text-decoration: none;
}

body {
    background-size: cover;
    background-position: center;
    background-attachment: fixed;
    margin: 0;
    padding: 0;
    overflow: auto; 
